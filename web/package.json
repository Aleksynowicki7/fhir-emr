{
    "name": "web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@craco/craco": "^6.4.3",
        "@sentry/browser": "^6.16.1",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/classnames": "^2.3.1",
        "@types/jest": "^27.0.3",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/lodash": "4.14.178",
        "aidbox-react": "^1.3.3",
        "antd": "^4.16.13",
        "classnames": "^2.3.1",
        "craco-antd": "^1.19.0",
        "craco-babel-loader": "^0.1.4",
        "node-sass": "^6.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0",
        "react-scripts": "^4.0.3",
        "shared": "0.0.1",
        "ts-jest": "^27.1.1",
        "typescript": "^4.5.4",
        "query-string": "^7.0.1",
        "lodash": "4.17.21"
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "react-scripts test --watchAll=false",
        "eject": "react-scripts eject",
        "typecheck": "tsc",
        "lint": "eslint src/ --quiet"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{ts,tsx}",
            "!**/node_modules/**",
            "!**/contrib/**"
        ],
        "coverageReporters": [
            "text-summary",
            "html"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!axios)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ]
    }
}
